<% content_for :stylesheet do -%>
<style type="text/css">
  div#antenna_navi span.item {
    border-right: 1px solid #555555;
    padding-right: 3px;
    margin-right: 7px;
  }
</style>
<% end -%>

<%= render :partial => 'contents_left_base' %>

<% content_for :contents_right do -%>
  <% if @entries and @entries.respond_to?(:previous_page) %>
    <div class="navi ui-corner-all"><%= will_paginate_link(@entries) %></div>
  <% end %>
  <%= render :partial => "page_space",
             :locals => { :id_name => @id_name,
                          :title_name => @title_name,
                          :pages => @entries,
                          :symbol2name_hash => @symbol2name_hash } %>

  <% if @entries and @entries.respond_to?(:previous_page) %>
    <div class="navi ui-corner-all"><%= will_paginate_link(@entries) %></div>
  <% end %>
<% end -%>
