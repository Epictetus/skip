<div id="contents_container">
  <div id="contents_left">
    <%= render(:partial => 'user/portrait_box', :locals => { :current_user_info => @current_user_info }) %>
    <%= render :partial => 'antenna_box', :locals => { :system_antennas => @system_antennas, :my_antennas => @my_antennas } %>
  </div><!-- contents_left -->

  <div id="contents_right">
    <table width="100%"><tr>
    <td align="left"><%= link_to _("Prev Day (%s)") % @prev_day.strftime('%Y/%m/%d'), :year => @prev_day.year, :month => @prev_day.month, :day => @prev_day.day if @prev_day -%></td>
    <td align="right"><%= link_to _("Next Day (%s)") % @next_day.strftime('%Y/%m/%d'), :year => @next_day.year, :month => @next_day.month, :day => @next_day.day if @next_day -%></td>
    </tr></table>

    <%= render :partial => "page_space",
               :locals => { :id_name => 'entries',
                            :title_name =>  n_("%{wday}, %{date} - %{count} Entry", "(%{wday}) - %{count} Entries", @entries.size) % {:date => @selected_day.strftime(_("%B %d %Y")), :wday => _(SkipUtil::WDAYS[@selected_day.wday]), :count => @entries.size},
                            :pages => @entries} %>

    <table width="100%"><tr>
    <td align="left"><%= link_to _("Prev Day (%s)") % @prev_day.strftime('%Y/%m/%d'), :year => @prev_day.year, :month => @prev_day.month, :day => @prev_day.day if @prev_day -%></td>
    <td align="right"><%= link_to _("Next Day (%s)") % @next_day.strftime('%Y/%m/%d'), :year => @next_day.year, :month => @next_day.month, :day => @next_day.day if @next_day -%></td>
    </tr></table>
  </div><!-- contents_right -->

  <div id="contents_bottom"></div><!-- contents_bottom -->
</div><!-- contents_container -->
