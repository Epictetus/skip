<div id="collaboration_apps">
  <%= show_title_bar("rss", _("Update info"), {}) %>
  <%= skip_image_tag 'indicator.gif' %>
</div>

<% content_for :javascript_initializers do -%>
  $j.ajax({
      url: '<%= url_for(:controller => 'mypage', :action => 'collaboration_apps') %>',
      success: function(html) {
          $j('div#collaboration_apps').html(html);
      }
  });
<% end -%>

