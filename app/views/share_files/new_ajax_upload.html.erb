<style type="text/css">
form.share_file_form label, form.share_file_form input {
  font-size: 10px;
  color: #555;
}
</style>

<% form_for @share_file, :url => polymorphic_path([current_tenant, current_target_owner, :share_files], :ajax_upload => 1), :html => {:multipart => true, :class => 'share_file_form', :target => IframeUploader::UPLOAD_KEY} do |f| -%>
  <%= render f %>
  <%= submit_tag _("Create"), :id => "submit_button" %>
<% end -%>
