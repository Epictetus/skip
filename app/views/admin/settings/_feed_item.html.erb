<span class="feed_item">
<% feed_setting_prefix = "settings[mypage_feed_settings][#{@index}]" -%>
<p><%= label Admin::Setting.name, :mypage_feed_settings_url %><%= text_field_tag "#{feed_setting_prefix}[url]", @feed_setting[:url], :id => 'settings_mypage_feed_settings_url' %></p>
<p><%= label Admin::Setting.name, :mypage_feed_settings_title %><%= text_field_tag "#{feed_setting_prefix}[title]", @feed_setting[:title], :id => 'settings_mypage_feed_settings_title' %></p>
</span>
