<%= render :partial => '/admin/contents_left' %>
<% content_for :contents_right do -%>
<h2><%= _('Listing %{model}') % {:model => _('openid identifier')} %></h2>

<div class="box tabular">
  <%= render :partial => '/admin/shared/sortable_table',
             :locals => {
               :columns => %w[url],
               :objects => @openid_identifiers,
               :class_name => 'Admin::OpenidIdentifier',
               :link_elem => [['Show','admin_account_openid_identifier_url(@account, object)'], ['Edit','edit_admin_account_openid_identifier_path(@account, object)'], ['Destroy','admin_account_openid_identifier_url(@account, object)',{:confirm => _('Are you sure?'), :method => :delete}]]
             }
  -%>
</div>

<%= link_to _('New %{model}') % {:model => _('openid identifier')}, new_admin_account_openid_identifier_path %> |
<%= link_to _('%{model} Show') % {:model => _('account')}, admin_account_path(@account) %>
<% end -%>
