<%= render :partial => '/admin/contents_left' %>
<% content_for :contents_right do -%>
<h1><%= _('New %{model}') % {:model => _('openid identifier')} %></h1>

<% form_for(@openid_identifier, :url => admin_account_openid_identifiers_url(@account)) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit _('Create') %>
  </p>
<% end %>

<%= link_to _('Back'), admin_account_openid_identifiers_path %>
<% end -%>
