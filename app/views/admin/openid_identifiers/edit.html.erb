<%= render :partial => '/admin/contents_left' %>
<% content_for :contents_right do -%>
<h2><%= _('Editing %{model}') % {:model => _('openid identifier')} %></h2>
<div class="box tabular">
<% form_for(@openid_identifier, :url => admin_account_openid_identifier_url(@account, @openid_identifier), :method => :put) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit _('Update') %>
  </p>
<% end %>
</div>
<%= link_to _('Show'), admin_account_openid_identifier_url(@account, @openid_identifier) %> |
<%= link_to _('Back'), admin_account_openid_identifiers_path(@account) %>
<% end -%>
