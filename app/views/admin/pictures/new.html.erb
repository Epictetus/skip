<% content_for :topic_path do -%>
<%= render :partial => '/admin/shared/topic_path' %>
<% end -%>

<div class="box">
  <% form_for(@picture, :url => admin_tenant_user_pictures(current_target_group, @user), :html => {:multipart => true}) do |f| -%>
    <%= render f %>
    <p>
      <%= f.submit _('Upload'), :disable_with => _('Now Uploading...') %>
    </p>
  <% end -%>
</div>
